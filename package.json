{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.12",
  "author": "LekoArts <hello@lekoarts.de>",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "npx gatsby build",
    "develop": "npx gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "npx gatsby develop",
    "serve": "npx gatsby serve",
    "clean": "npx gatsby clean",
    "deploy": "npx gatsby build && gh-pages -d public -r https://git:${access-token}@github.com/${GITHUB_REPOSITORY}.git"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^6.2.6",
    "gatsby": "^5.14.6",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "highlight.js": "^11.11.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "ajv": "^8.17.1",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.9.2"
  }
}
